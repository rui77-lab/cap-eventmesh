{
  "name": "cap-sbg-eventmesh",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap-cloud-sdk/http-client": "^3.26.4",
    "@sap-cloud-sdk/resilience": "^3.26.4",
    "@sap/cds": "^8",
    "@sap/xb-msg-amqp-v100": "^0.9.58",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.9.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "sql": {
      "native_hana_associations": false
    },
    "requires": {
      "messaging": {
        "kind": "enterprise-messaging-shared",
        "queue": {
          "name": "$namespace/poc"
        }
      },
      "auth": "xsuaa",
      "db": "hana",
      "API_SERVICE_ORDER_SRV": {
        "kind": "odata-v2",
        "model": "srv/external/OP_API_SERVICE_ORDER_SRV_0001",
        "credentials": {
          "destination": "S48_HTTP",
          "path": "/sap/opu/odata/sap/API_SERVICE_ORDER_SRV"
        }
      },
      "c4codata": {
        "kind": "odata-v2",
        "model": "srv/external/ticket",
        "credentials": {
          "destination": "BASMigration",
          "path": "/sap/c4c/odata/v1/c4codataapi"
        }
      }
    }
  }
}